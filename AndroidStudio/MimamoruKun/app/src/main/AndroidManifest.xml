<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.slack.nagoyalab_sutra03.teamc.mimamorukun">

    <application

        android:name=".MyApplication"

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EventLogDetailActivity" />
        <activity android:name=".TemperatureEventActivity" />
        <activity android:name=".HandlingRequiredEventActivity"></activity>
        <service android:name="com.slack.nagoyalab_sutra03.teamc.mimamorukun.EventLog.EventLogStoreService" />
        <service android:name="com.slack.nagoyalab_sutra03.teamc.mimamorukun.Sensor.SensorService" />
    </application>

</manifest>